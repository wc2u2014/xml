    <bean id="cronTriggerNotificacionCreditosFinalizados"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
        <property name="jobDetail" ref="complexJobDetailNotificacionCreditosFinalizados" />
        <property name="cronExpression" value="${notificacioncreditosfinalizados.quartz.cronExpression}" />
    </bean>
    <!-- Scheduler factory bean to glue together jobDetails and triggers to Configure Quartz Scheduler -->
    <bean  class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
        <property name="jobDetails">
            <list>                
                <ref bean="complexJobDetailAvisosPago" />
                <ref bean="complexJobDetailNotificacionAvisosPagos" />
                <ref bean="complexJobDetailNotificacionCreditosNuevos" />
                <ref bean="complexJobDetailNotificacionMoras" />
                <ref bean="complexJobDetailRechazosPagos" />
                <ref bean="complexJobDetailNotificacionRechazosPagos" />
                <ref bean="complexJobDetailNotificacionCreditosFinalizados" />
            </list>
        </property>
 
        <property name="triggers">
            <list>               
                <ref bean="cronTriggerAvisosPago" />
                <ref bean="cronTriggerNotificacionAvisosPagos" />
                <ref bean="cronTriggerNotificacionCreditosNuevos" />
                <ref bean="cronTriggerNotificacionMoras" />
                <ref bean="cronTriggerRechazosPagos" />
                <ref bean="cronTriggerNotificacionRechazosPagos" />
                <ref bean="cronTriggerNotificacionCreditosFinalizados" />
            </list>
        </property>
    </bean>
</beans>