<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans classpath:xsd/spring-beans-4.0.xsd
                            http://www.springframework.org/schema/context classpath:xsd/spring-context-4.0.xsd">
 
	
	<context:property-placeholder
		ignore-resource-not-found="true"
		location="file:${PATH_PROPERTIES_INTEGRATOR}/GestorBloqueo/GestorBloqueo.properties" />
	<bean id="log4jInitialization"
		class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
		<property name="targetClass" value="org.springframework.util.Log4jConfigurer" />
		<property name="targetMethod" value="initLogging" />
		<property name="arguments">
			<list>
				<value>file:${PATH_PROPERTIES_INTEGRATOR}/GestorBloqueo/GestorBloqueo-log4j.xml</value>
			</list>
		</property>
	</bean>	
	<!-- AVISOS PAGOS  -->
    <bean name="genericProccessAvisosPago" class="co.com.claro.financialintegrator.implement.AvisosPagos">    	   	
    	<property name="pros">
    		<props>
    			<prop key="WSLConsultaConfiguracionBatchAddress">${wsdl.WSLConsultaConfiguracionBatchAddress}</prop>
    		    <prop key="WSLConsultaConfiguracionBatchTimeOut">${wsdl.WSLConsultaConfiguracionBatchTimeOut}</prop>
				<prop key="BatchName">BatchFinancialIntegratorAvisosPagos</prop>				
    		</props>
    	</property>
    </bean>
    <bean name="complexJobDetailAvisosPago"    class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
        <property name="jobClass" value="co.com.claro.financialintegrator.spring.quartz.ScheduledJob" />
        <property name="jobDataMap">
            <map>
                <entry key="genericProccess" value-ref="genericProccessAvisosPago" />
            </map>
        </property>
        <property name="durability" value="true" />
    </bean>
  
    <!-- Run the job every 5 seconds only on Weekends -->
    <bean id="cronTriggerAvisosPago"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
        <property name="jobDetail" ref="complexJobDetailAvisosPago" />
        <property name="cronExpression" value="${avisospagos.quartz.cronExpression}" />
    </bean>
    <!-- NOTIFICACION AVISOS PAGOS  -->
    <bean name="genericProccessNotificacionAvisosPagos" class="co.com.claro.financialintegrator.implement.NotificacionAvisosPagos">    	   	
    	<property name="pros">
    		<props>
    			<prop key="WSLConsultaConfiguracionBatchAddress">${wsdl.WSLConsultaConfiguracionBatchAddress}</prop>
    		    <prop key="WSLConsultaConfiguracionBatchTimeOut">${wsdl.WSLConsultaConfiguracionBatchTimeOut}</prop>
				<prop key="BatchName">BatchFinancialIntegratorNotificacionAvisosPagos</prop>				
    		</props>
    	</property>
    </bean>
    <bean name="complexJobDetailNotificacionAvisosPagos"    class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
        <property name="jobClass" value="co.com.claro.financialintegrator.spring.quartz.ScheduledJob" />
        <property name="jobDataMap">
            <map>
                <entry key="genericProccess" value-ref="genericProccessNotificacionAvisosPagos" />
            </map>
        </property>
        <property name="durability" value="true" />
    </bean>
     <!-- Run the job every 5 seconds only on Weekends -->
    <bean id="cronTriggerNotificacionAvisosPagos"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
        <property name="jobDetail" ref="complexJobDetailNotificacionAvisosPagos" />
        <property name="cronExpression" value="${notificacionavisospagos.quartz.cronExpression}" />
    </bean>
   <!-- NOTIFICACION CREDITOS NUEVOS  -->
