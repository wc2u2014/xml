    		    <prop key="WSLConsultaConfiguracionBatchTimeOut">${wsdl.WSLConsultaConfiguracionBatchTimeOut}</prop>
				<prop key="BatchName">BatchFinancialIntegratorRechazosPagos</prop>				
    		</props>
    	</property>
    </bean>
    <bean name="complexJobDetailRechazosPagos"    class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
