    </bean>
    <!-- NOTIFICACION RECHAZOS PAGOS  -->
    <bean name="genericProccessNotificacionRechazosPagos" class="co.com.claro.financialintegrator.implement.NotificacionPagosRechazados">    	   	
    	<property name="pros">
    		<props>
