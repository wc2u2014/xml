    </bean>
     <!-- NOTIFICACION CREDITOS FINLIZADOS PAGOS  -->
    <bean name="genericProccessNotificacionCreditosFinalizados" class="co.com.claro.financialintegrator.implement.NotificacionCreditosFinalizados">    	   	
    	<property name="pros">
    		<props>
    			<prop key="WSLConsultaConfiguracionBatchAddress">${wsdl.WSLConsultaConfiguracionBatchAddress}</prop>
    		    <prop key="WSLConsultaConfiguracionBatchTimeOut">${wsdl.WSLConsultaConfiguracionBatchTimeOut}</prop>
				<prop key="BatchName">BatchFinancialIntegratorNotificacionCredFinal</prop>				
    		</props>
    	</property>
    </bean>
    <bean name="complexJobDetailNotificacionCreditosFinalizados"    class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
        <property name="jobClass" value="co.com.claro.financialintegrator.spring.quartz.ScheduledJob" />
        <property name="jobDataMap">
            <map>
                <entry key="genericProccess" value-ref="genericProccessNotificacionCreditosFinalizados" />
            </map>
